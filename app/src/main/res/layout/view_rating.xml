<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:padding="@dimen/dimen_10">

        <TextView
            android:id="@+id/view_rating_textview_satification"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:text="@string/lbl_satisfaction"
            android:textColor="@color/white"
            android:textSize="@dimen/text_normal_app" />

        <RatingBar
            android:id="@+id/view_rating_rating_satification"
            style="@style/customRatingBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/view_rating_textview_satification"
            android:layout_alignTop="@+id/view_rating_textview_satification"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/dimen_15"
            android:layout_toEndOf="@+id/view_rating_textview_satification"
            android:isIndicator="true"
            android:numStars="5"
            android:paddingTop="@dimen/dimen_2"
            android:stepSize="1.0" />

        <!--</RelativeLayout>-->

        <!--<RelativeLayout-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_marginTop="@dimen/dimen_5"-->
        <!--android:gravity="center_vertical">-->

        <TextView
            android:id="@+id/view_rating_textview_commitment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/view_rating_textview_satification"
            android:gravity="end"
            android:text="@string/lbl_commitment"
            android:textColor="@color/white"
            android:textSize="@dimen/text_normal_app" />

        <RatingBar
            android:id="@+id/view_rating_rating_commitment"
            style="@style/customRatingBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/view_rating_textview_commitment"
            android:layout_alignStart="@+id/view_rating_rating_satification"
            android:layout_alignTop="@+id/view_rating_textview_commitment"
            android:layout_below="@+id/view_rating_textview_satification"
            android:layout_gravity="center_vertical"
            android:isIndicator="true"
            android:numStars="5"
            android:paddingTop="@dimen/dimen_2"
            android:rating="3"
            android:stepSize="1.0" />

    </RelativeLayout>
</LinearLayout>